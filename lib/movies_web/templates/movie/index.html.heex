<h1>Listing Movies</h1>

<table>
  <thead>
    <tr>
      <th>Movie</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
  <%= for movie <- @movies do %>
      <tr>
        <td><%= movie.movie_id %></td>

        <td>
          <span><%= link "Show", to: Routes.movie_path(@conn, :show, movie) %></span>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>

